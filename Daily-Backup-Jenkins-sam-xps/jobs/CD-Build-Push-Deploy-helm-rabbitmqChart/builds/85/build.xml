    stage(&apos;Checkout SCM&apos;) {
      steps {
        container(&apos;helm&apos;) {
            // clone master and then checkout directory rabbutmq-chart from master branch
            sh &apos;git clone https://github.com/samer-habash/kubernetes-project-helm-charts&apos;
            dir(&apos;kubernetes-project-helm-charts&apos;) {
                sh(&apos;&apos;&apos;
                git checkout gh-pages
                git checkout master rabbitmq-chart/
                &apos;&apos;&apos;)
